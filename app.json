{
    "expo": {
        "name": "Roughapp",
        "slug": "Rough",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.rough"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.rough"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "react-native-vision-camera",
                {
                    "cameraPermissionText": "Rough needs access to your Camera.",
                    "enableMicrophonePermission": true,
                    "microphonePermissionText": "Rough needs access to your Microphone."
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "d3be7ce0-7bb5-4cac-a765-d16a685a4c90"
            }
        }
    }
}
